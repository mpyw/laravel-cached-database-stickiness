{
    "name": "mpyw/laravel-cached-database-stickiness",
    "description": "Guarantee database stickiness over the same user's consecutive requests",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": [
        "laravel",
        "illuminate",
        "database",
        "sticky",
        "stickiness",
        "request",
        "read",
        "write"
    ],
    "autoload": {
        "psr-4": {
            "Mpyw\\LaravelCachedDatabaseStickiness\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\LaravelCachedDatabaseStickiness\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "illuminate/container": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/queue": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=7.0",
        "phpunit/phpunit": ">=9.5",
        "mockery/mockery": "^1.3.3 || ^1.4.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\LaravelCachedDatabaseStickiness\\StickinessServiceProvider"
            ]
        }
    }
}
